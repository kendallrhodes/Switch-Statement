let x = new Date();
let day = x.getDate();
switch (day) {
	case 'Sunday':
		console.log('Today is my own and no one can take that away from me.');
		break;
	case 'Monday':
		console.log('Today is my NOT my own. Today I am a wage slave.');
		break;
	case 'Tuesday':
		console.log('Today is my NOT my own. Today I am STILL a wage slave.');
		break;
	case 'Wednesday':
		console.log('Today is my NOT my own. Today I am STILL a wage slave.');
		break;
	case 'Thursday':
		console.log('Today is my own NOT. Today I am STILL a wage slave.');
		break;
	case 'Friday':
		console.log(
			'Today is my own NOT but there is hope. The weekend begins tomorrow.'
		);
		break;
	case 'Saturday':
		console.log('Today is my own and no one can take that away from me.');
		break;
	default:
		console.log("I'm going back to bed");
}
